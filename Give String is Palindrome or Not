class Solution {
	
	
	public static boolean isPlaindrome(String s )
	{
		
		s = s.toLowerCase();
	     
	  int left = 0;
	  int right = s.length()-1;
	  
	  while(left < right){
	  
	  if(s.charAt(left) != s.charAt(right)){
	  
	        return false;
	  
	  }
	     left++;
	     right--;
	  }
	       return true;
	}

	 public static void main(String []arg){
	 
	   String s = "NiTin";
		
		if(isPlaindrome(s)){
		     
			 System.out.println("Palindrome");
		
		}
		else{
		      System.out.println("no");
		}
	 
	 
	 }
    
}
